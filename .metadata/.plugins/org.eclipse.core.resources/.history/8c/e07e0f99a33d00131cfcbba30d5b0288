package modelosDeClasse;

public class Funcionario extends Pessoa{

	private int cargaHorariaSemanal;
	private Double salario;
	private int anoDeContratacao;
	private int horasExtras;
	
	public Funcionario(String nome, String telefone, Endereco endereco, int cargaHorariaSemanal, Double salario, int anoDeContratacao) {
		super(nome, telefone, endereco);
		this.cargaHorariaSemanal = cargaHorariaSemanal;
		this.salario = salario;
		this.anoDeContratacao = anoDeContratacao;
	}
	
	public int getCargaHorariaSemanal() {
		return cargaHorariaSemanal;
	}

	public void setCargaHorariaSemanal(int cargaHorariaSemanal) {
		this.cargaHorariaSemanal = cargaHorariaSemanal;
	}

	public Double getSalario() {
		return salario;
	}

	public void setSalario(Double salario) {
		this.salario = salario;
	}

	public int getAnoDeContratacao() {
		return anoDeContratacao;
	}

	public void setAnoDeContratacao(int anoDeContratacao) {
		this.anoDeContratacao = anoDeContratacao;
	}

	public int getHorasExtras() {
		return horasExtras;
	}

	public void setHorasExtras(int horasExtras) {
		this.horasExtras = horasExtras;
	}

	
}
